<%= link_to "本を探す", book_show_form_path, remote: true %>
<div class="page_wrapper">
    <div id="search_book_form"></div>
    <div class="book_img_box">
        <% if @books.present? %>
            <% @books.each do |shelf| %>
                <%= form_with(model: @book, url: "book/#{shelf.isbn}") do |f| %>
                    <%= f.hidden_field shelf.isbn %>
                    <button>
                        <%= image_tag('book_img.svg', class:'book_img') %>
                    </button>
                    
                    <div class="book_title">本のタイトル</div>
                    <%= shelf.user_id %>
                    <%=  %>
                <% end %>
            <% end %>
        <% else %>
            <p>本が登録されていません。</p>
        <% end %>
    </div>
</div>


